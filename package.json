{
  "name": "job-finder",
  "version": "1.0.0",
  "description": "#### Track Your Job Search",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "setup-production": "npm run install-client && npm run build-client && npm install && npx update-browserslist-db@latest && npm install react-icons --save",
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build",
    "server": "nodemon server --ignore client",
    "client": "npm start --prefix client",
    "start": "concurrently --kill-others-on-fail \" npm run server\" \" npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viczdev/job-finder.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viczdev/job-finder/issues"
  },
  "homepage": "https://github.com/viczdev/job-finder#readme",
  "devDependencies": {
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.19"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "http-proxy-middleware": "^2.0.6",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^6.4.4",
    "morgan": "^1.10.0",
    "react-icons": "^4.7.1",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "engines": {
    "node": "16.15"
  }
}
